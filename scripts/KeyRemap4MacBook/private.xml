<?xml version="1.0"?>
<root>
    <item>
        <name>Command+Backqute to KANA/EISUU (toggle)</name>
        <identifier>private.jis_commandBackquote2toggle_kana_eisuu_not_vm</identifier>
        <not>VIRTUALMACHINE</not>
        <autogen>
          __KeyToKey__
          KeyCode::BACKQUOTE, VK_COMMAND,
          KeyCode::VK_JIS_TOGGLE_EISUU_KANA
        </autogen>
    </item>
    <item>
        <name>Option+L to Sleep Displays</name>
        <identifier>private.option_l_to_sleep_displays</identifier>
        <not>VIRTUALMACHINE</not>
        <autogen>
          __KeyToKey__
          KeyCode::L, VK_OPTION,
          KeyCode::VK_CONSUMERKEY_EJECT, ModifierFlag::CONTROL_L | ModifierFlag::SHIFT_L
        </autogen>
    </item>
</root>
