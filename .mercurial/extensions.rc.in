# -*- coding:utf-8; mode:conf; -*-
# extensions.rc

[extensions]
collapse = $HOME/.mercurial/extensions/hgcollapse/hgext/collapse.py
flow = $HOME/.mercurial/extensions/hgflow/src/hgflow.py
hggit = $HOME/.mercurial/extensions/hg-git/hggit
hgsubversion = $HOME/.mercurial/extensions/hgsubversion/hgsubversion
review = $HOME/.mercurial/extensions/hg-review/review
grepfile = $HOME/.mercurial/extensions/hg-grepfile/grepfile.py
